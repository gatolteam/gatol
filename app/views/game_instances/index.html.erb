<h1>Listing game_instances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_instances.each do |game_instance| %>
      <tr>
        <td><%= link_to 'Show', game_instance %></td>
        <td><%= link_to 'Edit', edit_game_instance_path(game_instance) %></td>
        <td><%= link_to 'Destroy', game_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game instance', new_game_instance_path %>
